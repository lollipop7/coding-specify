<div class="leader">
    <div class="toptitle">
        <img src="images/leader/01.png" alt="领导团队">
    </div>
    <div class="pic-board">
        <div class="row-1" style="text-align: center">
            <img src="images/leader/row101.png" alt="李兴春">
        </div>
        <div class="row-2">
            <ul class="clearfix">
                <% for(var j=0; j< data.leaderData.row.length; j++){ %>
                <li class="piclist fl"  style="text-align: center">
                    <img src="images/leader/<%= data.leaderData.row[j] %>" alt="王佑常,李心丹,陆风">
                </li>
                <% } %>
            </ul>
        </div>
    </div>
</div>