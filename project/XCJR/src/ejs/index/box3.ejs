<div class="box3">
    <div class="toptitle">
        <img src="images/index/03.png" alt="新城金融大记事">
    </div>
    <ul>
        <% for(var i=0; i< data.indexData.box3.length; i++){ %>
            <li>
                <% if(i<=3){ %>
                    <% if(i%2==0){ %>
                    <div class="his-list left clearfix">
                    <% }else{ %>
                    <div class="his-list right clearfix">
                    <% } %>
                <% }else if(i==4){ %>
                    <div class="his-list justify clearfix">
                <% }else{ %>
                    <% if(i%2==0){ %>
                    <div class="his-list right clearfix">
                    <% }else{ %>
                    <div class="his-list left clearfix">
                    <% } %>
                <% } %>
                        <img src="images/index/<%= data.indexData.box3[i] %>" alt="">
                    </div>
                    <% if(i!==data.indexData.box3.length-1){ %>
                    <div class="line"></div>
                    <% } %>
            </li>
        <% } %>
    </ul>
</div>